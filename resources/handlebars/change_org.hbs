<br >
<div class="theForm" >

    <input type="hidden" name="reportType" id="reportType" value="change_org" >

    <div class="left" >
        <label >Name</label >
        <input type="text" class="inputRender" name="name" id="name" value="{{ data.fromAD.givenname }}" required >
        <span class="errorSpan" >*</span >
    </div >
    <div class="left" >
        <label >Last Name</label >
        <input type="text" class="inputRender" name="lastName" id="lastName" value="{{ data.fromAD.sn }}" required >
        <span class="errorSpan" >*</span >
    </div >

    <p ></p ><br ><br >

    <div class="left" >
        <label >Email</label >
        <input type="text" class="inputRender" name="newEmail" id="newEmail" value="{{ data.fromAD.mail }}" required >
        <input type="hidden" class="inputRender" name="email" id="email" value="{{ data.fromAD.mail }}" required >
        <span class="errorSpan" >*</span >
    </div >
    <div class="left" >
        <label >Employee ID #:</label >
        <input class="inputRender" name="employeeID" style="width: 80px" >
    </div >

    <p ></p ><br ><br >

    <div >
        <label >Title:</label >
        <input type="text" size="50%" name="title" class="inputRender" value="{{ data.fromAD.title }}" required="" >
        <span class="errorSpan" >*</span >
    </div >

    <p ></p ><br >
    <div class="left" >
        <label >Department</label >
        <select class="inputRender" name="department" id="department" >
            <option value="" >Select</option >
            {{#each data.departments }}
                <option >{{this}}</option >
            {{/each}}
        </select ><span id="departmentError" class="errorSpan" >*</span >
    </div >
    <div class="left" >

        <label >Company</label >
        <select class="inputRender" name="company" id="company" >
            <option value="" >Select</option >
            {{#each data.companies }}
                <option >{{this}}</option >
            {{/each}}
        </select ><span id="companyError" class="errorSpan" >*</span >
    </div >

    <p ></p ><br >

    <div class="left" >
        <label >Manager</label >
        <input type="text" name="manager" id="manager" class="inputRender" autocomplete="off"
               value="{{ data.fromAD.manager }}"
               required="" >
        <span id="searchProgressManager" ></span >
    </div >
    <div class="left" >
        <input type="hidden" name="managerEmail" id="managerEmail" value="{{ data.fromAD.managerEmail }}" >
    </div >

    <p ></p ><br >

    <div class="left" >
        <label >
            Effective Date</label >
        <input class="inputRender" name="effectiveDate" id="effectiveDate" style="width: 100px"
               required="" value="" placeholder="mm/dd/yyyy" >
        <span class="errorSpan" >*</span >

        <div id="termDateError" class="errorSpan" ></div >
    </div >
    <div class="left" >
        <label >Employee Status</label >
        <select class="inputRender" name="hireStatus" id="hireStatus" >
            <option value="" >Select</option >
            {{#each data.hireStatus }}
                <option >{{this}}</option >
            {{/each}}
        </select >
    </div >
